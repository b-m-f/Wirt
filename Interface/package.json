{
  "name": "@wirtbot/interface",
  "version": "2.7.3",
  "author": "b-m-f <max@ehlers.berlin>",
  "license": "AGPL-3",
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "build": "vue-cli-service build",
    "build:test": "vue-cli-service build --mode=test",
    "test": "vue-cli-service test:unit --passWithNoTests",
    "audit": "npm audit --production",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --mode=development",
    "dev-test": "vue-cli-service serve --mode=localtest",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/vue-fontawesome": "2.0.2",
    "@kazupon/vue-i18n-loader": "0.5.0",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@wasm-tool/wasm-pack-plugin": "1.4.0",
    "babel-eslint": "10.1.0",
    "core-js": "3.12.0",
    "eslint": "7.26.0",
    "eslint-plugin-vue": "7.9.0",
    "html-webpack-plugin": "5.3.1",
    "jszip": "3.6.0",
    "lodash-es": "4.17.21",
    "qrcode": "1.4.4",
    "sass": "1.32.12",
    "sass-loader": "10.1.1",
    "text-encoding": "0.7.0",
    "vue": "2.6.12",
    "vue-cli-plugin-i18n": "2.1.0",
    "vue-i18n": "8.24.4",
    "vue-loader": "15.9.6",
    "vue-markdown-loader": "2.4.1",
    "vue-router": "3.5.1",
    "vue-template-compiler": "2.6.12",
    "vuex": "3.6.2",
    "vuex-persistedstate": "3.2.0"
  },
  "devDependencies": {
    "@vue/test-utils": "1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "warn",
      "no-debugger": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": "6"
    },
    "overrides": [
      {
        "files": [
          "**/src/**/*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "Shared-Libs",
    "**/crate/*"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
