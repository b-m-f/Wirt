{
  "name": "wirt-interface",
  "version": "1.0.0",
  "author": "b-m-f <max@ehlers.berlin>",
  "license": "AGPL-3",
  "scripts": {
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit --passWithNoTests",
    "audit": "npm audit --production",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --mode=development",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/vue-fontawesome": "2.0.0",
    "@intlify/vue-i18n-loader": "^2.0.0-beta.1",
    "@vue/cli-plugin-babel": "4.5.7",
    "@vue/cli-plugin-eslint": "4.5.7",
    "@vue/cli-plugin-unit-jest": "4.5.7",
    "@vue/cli-service": "4.5.7",
    "@wasm-tool/wasm-pack-plugin": "1.3.1",
    "@wirtbot/config-generators": "file:../shared-libs/config-generators",
    "@wirtbot/crypto": "file:../shared-libs/crypto",
    "@wirtbot/rust-wasm": "file:../shared-libs/rust-wasm",
    "babel-eslint": "10.1.0",
    "core-js": "3.6.5",
    "eslint": "7.11.0",
    "eslint-plugin-vue": "7.0.1",
    "eslint-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "4.5.0",
    "jszip": "3.5.0",
    "lodash-es": "4.17.15",
    "qrcode": "1.4.4",
    "sass": "1.27.0",
    "sass-loader": "10.0.3",
    "text-encoding": "0.7.0",
    "vue": "2.6.12",
    "vue-cli-plugin-i18n": "1.0.1",
    "vue-i18n": "8.22.0",
    "vue-loader": "15.9.3",
    "vue-markdown-loader": "2.4.1",
    "vue-router": "3.4.6",
    "vue-template-compiler": "2.6.12",
    "vuex": "3.5.1",
    "vuex-persistedstate": "3.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "warn",
      "no-debugger": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": "6"
    },
    "overrides": [
      {
        "files": [
          "**/src/**/*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "shared-libs"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}